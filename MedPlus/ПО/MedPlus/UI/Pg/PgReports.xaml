<Page x:Class="MedPlus.UI.Pg.PgReports"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedPlus.UI.Pg"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PgReports">

    <StackPanel>
        <Button x:Name="BtnOpenSignalStatement" Style="{StaticResource BtnWhite}" Click="BtnOpenSignalStatement_Click">Сигнальная ведомость</Button>
        <Button x:Name="BtnOpenListOfAdmitted" Style="{StaticResource BtnWhite}" Click="BtnOpenListOfAdmitted_Click">Списки поступивших</Button>
    </StackPanel>
</Page>
